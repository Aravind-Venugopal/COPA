
/*------------------------------------------------------------------

[All Add Ons - Common Stylesheet]

    Version:       4.5
    Primary Use:   Provide styles for common elements found all add-ons

------------------------------------------------------------------

[Table of Contents]

    1.  $Generics
    2.  $Tag Selector Page
    3.  $Edit Mode Sample Data
    4.  $Validator Callout
    5.  $Widget Layout Validation Error
    6.  $Processing Modal

------------------------------------------------------------------*/


/* [ 1. $Generics ]
/*******************************/
.clear { clear: both; font-size: 0; line-height: 0; overflow: hidden; }

/* Fix inconsistencies with page layouts and sidebars */
.layout-content[class*="sidebar-left-content-right"] .layout-region.content .content-fragment, 
.layout-content[class*="sidebar-left-content-center"] .layout-region.content .content-fragment
{ margin-left: 0; }

.layout-content[class*="sidebar-left-content-right"] .layout-region.left-sidebar .content-fragment,
.layout-content[class*="sidebar-left-content-center"] .layout-region.left-sidebar .content-fragment 
{ margin-right: 10px !important; }


/* [ 2. $Tag Selector Page ]
/*******************************/
#tagselector-page .field-list { padding-top: 8px; }
#tagselector-page .field-list .field-item-input table { width: 100%; }
    #tagselector-page .field-list .field-item-input table td { padding: 4px 0; }
        #tagselector-page .field-list .field-item-input table td input[type="checkbox"] { float: left; margin-top: 0; margin-left: 0; }
        #tagselector-page .field-list .field-item-input table td label { display: block; padding-left: 20px; padding-right: 20px; line-height: 16px; }

#tagselector-page .field-item-input .internal-link.cancel { margin-left: 0; }


/* [ 3. $Edit Mode Sample Data ]
/*******************************/
.content-fragment-management-content .sample-data-content { border: solid 1px; padding: 10px; }	
.content-fragment-management-content .sample-data-note { clear: both; }
    .content-fragment-management-content .sample-data-note .bottom { height: 5px; }
        .content-fragment-management-content .sample-data-note .bottom .r1,
        .content-fragment-management-content .sample-data-note .bottom .r2,
        .content-fragment-management-content .sample-data-note .bottom .r3,
        .content-fragment-management-content .sample-data-note .bottom .r4,
        .content-fragment-management-content .sample-data-note .content
        { background-color: #ffffaa; border-left: solid 1px; border-right: solid 1px; height: 1px; overflow: hidden; }

        .content-fragment-management-content .sample-data-note .bottom .r1 { margin: 0 1px; height: 2px; }
        .content-fragment-management-content .sample-data-note .bottom .r2 { margin: 0 2px; }
        .content-fragment-management-content .sample-data-note .bottom .r3 { margin: 0 3px; border-left-width: 2px; border-right-width: 2px; }
        .content-fragment-management-content .sample-data-note .bottom .r4 { margin: 0 5px; height: 0; border-top: solid 1px; border-left-width: 0; border-right-width: 0; }
    
    .content-fragment-management-content .sample-data-content,
    .content-fragment-management-content .sample-data-note .content,
    .content-fragment-management-content .sample-data-note .bottom .r1,
    .content-fragment-management-content .sample-data-note .bottom .r2,
    .content-fragment-management-content .sample-data-note .bottom .r3,
    .content-fragment-management-content .sample-data-note .bottom .r4
    { border-color: #eec600; }
    
    .content-fragment-management-content .sample-data-note .content { background: #ffffaa url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/AllAddOns/Icons/icon-sample-data-arrow.gif") 7px center no-repeat; color: #b79801; font-size: 10px; font-style: italic; height: auto; padding: 5px 10px 1px 20px; }


/* [ 4. $Validator Callout ]
/******************************/
input[type="text"].error,
input[type="password"].error,
input[type="input"].error,
textarea.error,
select.error
{ border: solid 1px #f00 !important; }

.validator-callout * { color: #fff !important; line-height: 130% !important; }
.validator-callout-inner { background: #f00 url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/AllAddOns/Icons/icon-validator-callout.gif") 13px 12px no-repeat !important; min-height: 20px; padding: 16px 15px 10px 45px !important; }
.validator-callout-inner:after { clear: both; content: "."; display: block; height: 0; visibility: hidden; }
    .validator-callout-inner .error-message { text-align: left; }
        .validator-callout-inner .error-message span { display: block; padding-bottom: 5px; }  

    .validator-callout-close { display: block; position: relative; }
        .validator-callout-close span { background: url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/AllAddOns/Icons/icon-validator-close.gif"); cursor: pointer; display: block; height: 6px; position: absolute; right: -8px; top: -10px; width: 6px; }
    
    .validator-callout-pointer { border-top: 10px solid #f00; border-right: 10px solid transparent; bottom: -10px !important; height: 0 !important; left: 10px !important; overflow: hidden; position: absolute !important; width: 0 !important; }
    .alternate .validator-callout-pointer { border-left: 10px solid transparent; border-right: none;  left: auto !important; right: 10px; } 


/* [ 5. $Widget Layout Validation Error ]
/******************************/
.admin-bar .navigation-item .internal-link.save-page.widget-layout-validation-error { background-position: right -72px; }
    .admin-bar .navigation-item .internal-link.save-page.widget-layout-validation-error span { background-position: left -48px; }
	
.menu.save-page-content.widget-layout-validation-error { background-color: transparent; border: none; min-width: 250px; padding: 5px 0 0; white-space: nowrap; width: auto; } 
    .menu.save-page-content.widget-layout-validation-error .field-list-header { background: transparent url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/AllAddOns/Backgrounds/background-layout-validation-error-pointer.gif") no-repeat; height: 6px; position: absolute; right: 38px; top: 0; width: 11px; }    
    .menu.save-page-content.widget-layout-validation-error fieldset { background-color: #dd6667; border: solid 1px #320000; -webkit-border-radius: 4px; -moz-border-radius: 4px; border-radius: 4px; border-top-width: 1px; padding: 0 10px; }    
        .menu.save-page-content.widget-layout-validation-error .field-list { margin: 0; }
            .menu.save-page-content.widget-layout-validation-error .field-list li { background: transparent url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/AllAddOns/Icons/icon-error-sml.png") 0 11px no-repeat; }
                .menu.save-page-content.widget-layout-validation-error .field-list li a { border-top: dotted 1px #b54b4b; cursor: pointer; display: block; font-weight: normal; padding: 12px 0 12px 24px; }
                .menu.save-page-content.widget-layout-validation-error .field-list li a:hover { background: transparent url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/AllAddOns/Backgrounds/background-layout-validation-error-hover.png") center bottom no-repeat; text-decoration: none; }
                .menu.save-page-content.widget-layout-validation-error .field-list li:first-child a { border: none; }
       
.editmode-preview.widget-layout-validation-error .message { background: #d66 url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/AllAddOns/Icons/icon-error-lrg.png") 10px 13px no-repeat; padding-left: 106px; }                    
    .editmode-preview.widget-layout-validation-error .message * { color: #fff; }
    .editmode-preview.widget-layout-validation-error .message h2 { font-size: 18px; margin: 0; padding: 10px 0; }

    /* lt-large Content Regions */        
    .lt-large .editmode-preview.widget-layout-validation-error .message { background-position: center 10px; padding-top: 96px; padding-left: 10px; }        
        .lt-large .editmode-preview.widget-layout-validation-error .message h2 { padding-top: 5px; }


/* [ 6. $Processing Modal ]
/******************************/
.custom-modal-container.processing-message .message-content { background: transparent url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/AllAddOns/Backgrounds/background-processing.gif") right 21px no-repeat; color: #333; font-size: 28px; font-weight: bold; margin: 0; padding: 0 25px 0 8px; }

/*------------------------------------------------------------------

[Activity Story Stream - Common Stylesheet]

    Version:       4.5
    Primary Use:   Provide common styles for the activity story stream widget and modal that are used in more that one add-on

------------------------------------------------------------------

[Table of Contents]

    1.  $Stream - Content-Item
    2.  $Stream - Filter
    3.  $Stream - Post Moderate
    4.  $Modal
    5.  $Modal - Details
    6.  $Modal - Post Moderate
    7.  $Modal - Actions
    8.  $Modal - Comments

------------------------------------------------------------------*/


/* [ 1. $Stream - Content-Item ]
/******************************/
.content-fragment.activity-story-stream .content-list { min-height: 0; }
.content-fragment.activity-story-stream .avatar { border: solid 1px #d6d6d6; min-height: 0; }
    .content-fragment.activity-story-stream .avatar:after { clear: both; content: "."; display: block; height: 0; overflow: hidden; visibility: hidden; }
    .content-fragment.activity-story-stream .avatar a { border: none !important; }

.content-fragment.activity-story-stream .post-attachment-viewer { padding: 5px; }
.content-fragment.activity-story-stream .content-item.activity-story { padding-left: 60px; }
.content-fragment.activity-story-stream .post-author .avatar { font-size: 0; left: -60px; line-height: 0; }
    .content-fragment.activity-story-stream .post-author .avatar img { border-radius: 0 !important; }

.content-fragment.activity-story-stream .post-content .activity-avatar { font-size: 0; line-height: 0; }
    .content-fragment.activity-story-stream .post-content .activity-avatar .avatar { border: none; }
    
    /* Fixes linked media from expanding parent in ie7 and ie8 */
    .lt-ie9 .content-fragment.activity-story-stream .post-content .activity-avatar { border: none; padding: 0; }
        .lt-ie9 .content-fragment.activity-story-stream .post-content .activity-avatar img { border: 1px solid #ccc; padding: 5px; }   

.activity-story .content-list.comments .comment-form.with-avatar .avatar img { display: block; border-radius: 0; }


/* [ 2. $Stream - Filter ]
/******************************/
.content-fragment.activity-story-stream .filter-option { background: url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/ActivityStoryStream/Backgrounds/background-filter-separator.png") no-repeat right 8px; }


/* [ 2. $Stream - Post Moderate ]
/******************************/
.content-fragment.activity-story-stream .content-item .post-content.activity { padding-right: 24px; }
.content-fragment.activity-story-stream .content-item > .full-post > .post-moderate { float: none; position: absolute; right: 0; }
.content-fragment.activity-story-stream .content-item:hover .post-moderate a, 
.content-fragment.activity-story-stream .content-item .post-moderate a.open 
{ display: none !important; }

    .content-fragment.activity-story-stream .content-item:hover > .full-post > .post-moderate a,
    .content-fragment.activity-story-stream .content-item > .full-post > .post-moderate a.open
    { display: inline-block !important; }
    
        .ie7 .content-fragment.activity-story-stream .content-item .post-moderate { margin-top: 0; } /* Adds extra space above the moderation actions in ie7 */
        .ie7 .content-fragment.activity-story-stream .content-item:hover > .full-post > .post-moderate a,
        .ie7 .content-fragment.activity-story-stream .content-item > .full-post > .post-moderate a.open
        { display: block !important; } /* Fixes display issues with the moderation menu in ie7 */
        

/* [ 3. $Modal ]
/******************************/
.theater-mask { background: transparent url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/ActivityStoryStream/Backgrounds/background-modal-mask.png") repeat; /* Opacity */ zoom: 1; filter: alpha(opacity=100); opacity: 1; }
