.sf-menu a.sf-with-ul { min-width: 1px; } /* Trigger ie7 hasLayout so spans position accurately */
.sf-sub-indicator { position: absolute; display: block; right: .75em; top: 1.05em; /* ie6 only */ width: 10px; height: 10px; text-indent: -999em; overflow: hidden; background: url("/Couto/AddOns/MightyMenu/Themes/998320880be54a2a88d394ee0abedc9f/images/arrows-ffffff.png") no-repeat -10px -100px; } /* 8-bit indexed alpha png. IE6 gets solid image only */
