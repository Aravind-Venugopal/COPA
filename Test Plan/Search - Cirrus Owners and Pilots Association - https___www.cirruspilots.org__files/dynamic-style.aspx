    .theater-container a.theater-back { background: url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/ActivityStoryStream/Buttons/button-activity-modal-back.png") left center no-repeat; left: -465px; }  
        .opacity .theater-container a.theater-next,
        .opacity .theater-container a.theater-back
