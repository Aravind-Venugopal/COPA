.theater-container a.theater-close:link, 
.theater-container a.theater-close:visited, 
.theater-container a.theater-close:hover,
.theater-container a.theater-close:active
{ background: url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/ActivityStoryStream/Buttons/button-activity-modal-close.png") no-repeat; height: 28px; right: -27px; top: -16px; width: 29px; z-index: 100; text-indent: -9999px; }

