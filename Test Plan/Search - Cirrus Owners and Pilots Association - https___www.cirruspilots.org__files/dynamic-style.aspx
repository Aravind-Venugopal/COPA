        .theater-container .theater-content-container .theater-content { background: url("/Couto/AddOnFramework/Themes/998320880be54a2a88d394ee0abedc9f/Images/ActivityStoryStream/Backgrounds/background-activity-modal-column.png") bottom right no-repeat; overflow: hidden; margin-right: 10px; }
